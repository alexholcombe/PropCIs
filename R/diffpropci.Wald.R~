diffpropci.Wald.mp <-
function(b,c,n,conf.level)
{
  z  <- qnorm(1-(1-conf.level)/2)
  est <- c-b
  diff <- (c-b)/(n+2)
  sd <- sqrt((b+c)-(c-b)^2/(n))/(n)
  ll <- diff - z*sd
  ul <- diff + z*sd
  #if(ll < -1) ll = -1
  #if(ul > 1) ul = 1
  est <- (c - b)/ n
  cint <- c(ll, ul)
  attr(cint, "conf.level") <- conf.level
  rval <- list(conf.int = cint, estimate = est)
  class(rval) <- "htest"
  return(rval)
}
